        <header>
           <br><br><br>
            <h1>Parking Spot Rental Information</h1>
              <table class="homeowner-table"> 
                <tr>    
                    <td>     
                            <!-- Homeowner Parking Spot Form -->
                        <div class="homeowner-wall .col-lg-3">
                          <form method="post" action="/homeowner" class="parkingSpotForm">
    <!-- ADDRESS -->
                            <div>
                                <input name="address" type="address" class="form-control" placeholder="Address" required autofocus>
                            </div><br>
    <!-- Availability Date -->
                                      <div class="form-group">
                                        <div class='input-group date' id='datetimepicker12'>
                                            <input type='text' class="form-control" name="availability_date"/>
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                      </div>
    <!-- Rental Length -->
                            <div>
                                <select class="form-control" id="rentalLength" name="rental_length">
                                  <option name="weekly" value="1">Weekly</option>
                                  <option name="monthly" value="2">Monthly</option>
                                </select>
                            </div><br>
    <!-- Rental Price -->
                            <div>
                                  <label>Choose Daily Rental Price</label>
                                  <div class="rentalPrice">
                                      <div class="plusMinus">
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" data-value="decrease" data-target="#spinner" data-toggle="spinner">
                                              <span class="glyphicon glyphicon-minus"></span>
                                            </button>
                                        </span>
                                      </div>
                                      <div class="plusMinus">
                                          <input type="text" data-ride="spinner" id="spinner" class="form-control input-number" value="1" name="rental_price">
                                      </div>
                                      <div class="plusMinus">
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" data-value="increase" data-target="#spinner" data-toggle="spinner">
                                              <span class="glyphicon glyphicon-plus"></span>
                                            </button>
                                        </span>
                                      </div>
                                  </div>
                            </div>
    <!--  Hidden geolocation field -->
                            <input type="hidden" id="latitude" name="latitude" value="49.899" />
                            <input type="hidden" id="longitude" name="longitude" value="-119.401" />
                            <br><br>
                            <div>
                            <button type="submit" class="btn btn-default btn-xl wow tada">Submit</button>
                            </div>
                           </form> 
                        </div>  
                    </td>                 
                    <td>
                     <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCI8bTRZVfp9K-DER1MfqkYsOrPKiVxwYM  
                     &callback=initHomeMap"
                     async defer></script>
                      <script src="/public/libs/homemap.js"></script>
    <!-- Google Maps API INSERT HERE -->
                        <div class="table-center">
                         <div id="map-div-homeowner" class="bg-dark">
                          <div id="homeMap" onload="initHomeMap()"></div>

                         </div>
                        </div>
                    </td>
                </tr>          
              </table>      
        </header>